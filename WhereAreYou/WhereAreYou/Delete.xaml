<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WhereAreYou.Delete">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout x:Name="C">
                <Label x:Name="CLbl" Text="Course:"/>
                <Entry x:Name="CEntry"/>
            </StackLayout>
            <Picker x:Name="pckDay" Title="What Day?" HorizontalOptions="CenterAndExpand" SelectedIndexChanged="pckDay_SelectedIndexChanged">
                <Picker.Items>
                    <x:String>Mon</x:String>
                    <x:String>Tue</x:String>
                    <x:String>Wed</x:String>
                    <x:String>Thur</x:String>
                    <x:String>Fri</x:String>
                </Picker.Items>
             </Picker>

            <StackLayout x:Name="STSL" VerticalOptions="Center">
                <Label x:Name="STLbl" Text="Start Time:"/>
                <Entry x:Name="STEntry"/>
               </StackLayout>
            <StackLayout>
                <Button x:Name="Deletedata" Text="Delete" Clicked"Deletedata_Clicked"></Button>
            </StackLayout>
            

        </StackLayout>
       
        

    </ContentPage.Content>
</ContentPage>